<mvc:View
        controllerName="ilim.pdm2.vacation_planning.controller.PlanOverview"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        xmlns:f="sap.f"
        displayBlock="true"
        height="100%">

    <f:DynamicPage height="100%" toggleHeaderOnTitleClick="true" showFooter="true">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="График отпусков на 2017 г."/>
                </f:heading>
                <f:actions>
                </f:actions>
                <f:expandedContent>
                    <Button icon="sap-icon://arrow-down"
                            type="Transparent"/>
                </f:expandedContent>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader>
                <f:content>
                    <FlexBox 	direction="Row"
                                alignItems="Center"
                                justifyContent="End">
                        <items>
                            <FlexBox 	direction="Column"
                                        alignItems="End"
                                        class="sapUiLargeMarginEnd">
                                <items>
                                    <NumericContent
                                            value="2.5"
                                            scale="Дн."
                                            valueColor="Neutral"
                                            indicator="None" state="Loaded"
                                            withMargin="false" />
                                    <Label text = "Остаток за предыдущие периоды"/>
                                </items>
                            </FlexBox>
                            <FlexBox 	direction="Column"
                                        alignItems="End">
                                <items>
                                    <NumericContent
                                            value="28"
                                            scale="Дн."
                                            valueColor="Neutral"
                                            indicator="None" state="Loaded"
                                            withMargin="false" />
                                    <Label text = "Право на отпуск 2017 г."/>
                                </items>
                            </FlexBox>
                        </items>
                    </FlexBox>

                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <mvc:XMLView viewName="ilim.pdm2.vacation_planning.view.PlanCreate" height="100%"/>
        </f:content>
        <f:footer>
            <OverflowToolbar>
                <Button text="Маршрут согласования"/>
                <Button text="Комментарии к плану"/>
                <Button text="График отпусков"/>
                <Button text="История"/>
                <ToolbarSpacer/>
                <Button type="Accept" text="Отправить"/>
            </OverflowToolbar>
        </f:footer>
